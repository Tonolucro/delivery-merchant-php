{
  "name": "tonolucro/delivery-merchant-v1-php",
  "description": "Biblioteca de integração com a //API Tonolucro Merchant",
  "keywords": ["tonolucro", "delivery", "api"],
  "homepage": "https://tonolucro.com/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Tonolucro",
      "homepage": "https://tonolucro.com/"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "lib-curl": "*",
    "lib-openssl": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Tonolucro\\Delivery\\Merchant\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload-dev":      {
    "psr-4":                 {
      "Tonolucro\\Delivery\\Merchant\\Tests\\": "tests/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  }
}
